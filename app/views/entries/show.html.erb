<% if flash[:notice] %>
<div class="notice"><%= flash[:notice] %></div>
<% end %>
<br>
<p>
  <strong>First Name:</strong>
  <%= @entry.first_name %>
</p>
<p>
  <strong>Last Name:</strong>
  <%= @entry.last_name %>
</p>
<p>
  <strong>E-mail Address:</strong>
  <%= @entry.email_address %>
</p>
<p>
  <strong>Gravatar Image:</strong>
  <%= image_tag avatar_url(@entry) %>
</p>
<%= link_to 'Back', entries_path %>
